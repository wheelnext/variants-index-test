                    "80_real",
                    "86_real",
                    "90_real",
                    "100_real",
                    "120_real"
                ]
            },
            "priority": {
                "order": [
                    "2"
                ]
            }
        },
        "cuda13.0": {
            "nvidia": {
                "cuda_version_lower_bound": [
                    "13.0"
                ],
                "sm_arch": [
